				<xs:element name="BANKAGREEMENT" type="BSString" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Соглашения с банком</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="CREDITBANKNAME" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Название банка кредита</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="80"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CREDITBANKSWIFT" minOccurs="0">
					<xs:annotation>
						<xs:documentation>SWIFT-код банка зачисления валюты</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CREDITBANKPLACE" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Город банка кредита</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CREDITBANKCORRACC" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Кор. счет банка кредита</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CREDITBANKPLACETYPE" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Тип населенного пункта банка кредита</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="VALIDITYPERIOD" type="BSDate" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Срок действия заявки</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="PAYMENTDETAILS" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Детали платежа, доп. информация</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="CHARGEACCOUNT" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Счет комиссии</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="VALUEDATETYPE" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Выбор даты валютирования</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="40"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="GROUNDRECEIPTSBLOB" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Документы, обосновывающие сделку</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Record" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="AMOUNT" type="BSMoney" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Сумма представленного документа</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DESCRIPTION" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Примечание</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="BSString">
													<xs:maxLength value="255"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="DOCUMENTDATE" type="BSDate" minOccurs="0">
											<xs:annotation>
												<xs:documentation>Дата представленного документа</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="DOCUMENTNUMBER">
											<xs:annotation>
												<xs:documentation>Номер представленного документа</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="BS_String">
													<xs:maxLength value="50"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="DOCUMENTTYPE">
											<xs:annotation>
												<xs:documentation>Тип представленного документа</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="BS_String">
													<xs:maxLength value="255"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CODEMESSAGE" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Код сообщения</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="50"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="MESSAGEFORBANK" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Сообщение для банка</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="NONACCEPTAGREEDATE" type="BSDate" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Дата соглашения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="NONACCEPTAGREENUMBER" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер соглашения</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BALANCEACCOUNT">
					<xs:annotation>
						<xs:documentation>Счет зачисления неиспользованной суммы</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BS_Integer">
							<xs:totalDigits value="25"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BALANCEBANKBIC" minOccurs="0">
					<xs:annotation>
						<xs:documentation>БИК банка зачисления неиспользованной суммы</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="15"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BALANCEDEPINFO" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Реквизиты банка для зачисления неиспользованной суммы</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BSString">
							<xs:maxLength value="255"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="BALANCETO">
					<xs:annotation>
						<xs:documentation>Признак «Неиспользованную валюту вернуть на счет в нашем банке / в другой кредитной организации»</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="BS_Integer">
							<xs:totalDigits value="1"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:simpleType name="BS_Date">
		<xs:annotation>
			<xs:documentation>Дата. DD.MM.YYYY</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
			<xs:pattern value="(0[1-9]|[12][0-9]|3[01])\.(0[1-9]|1[012])\.((19)|(20)\d{2})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BS_Double">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BS_Integer">
		<xs:restriction base="xs:decimal">
			<xs:fractionDigits value="0"/>
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BS_Money">
		<xs:restriction base="xs:double">
			<xs:minInclusive value="0"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BS_String">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BS_STRING">
		<xs:annotation>
			<xs:documentation>Пустая строка</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="0"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="BSDate">
		<xs:annotation>
			<xs:documentation>DD.MM.YYYY | ""</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:list itemType="BS_Date"/>
			</xs:simpleType>
			<xs:simpleType>
				<xs:list itemType="BS_STRING"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="BSDouble">
		<xs:annotation>
			<xs:documentation>DOUBLE | ""</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:list itemType="BS_Double"/>
			</xs:simpleType>
			<xs:simpleType>
				<xs:list itemType="BS_STRING"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="BSInteger">
		<xs:annotation>
			<xs:documentation>INTEGER | ""</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:list itemType="BS_Integer"/>
			</xs:simpleType>
			<xs:simpleType>
				<xs:list itemType="BS_STRING"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="BSMoney">
		<xs:annotation>
			<xs:documentation>MONEY | ""</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:list itemType="BS_Money"/>
			</xs:simpleType>
			<xs:simpleType>
				<xs:list itemType="BS_STRING"/>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
	<xs:simpleType name="BSString">
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
